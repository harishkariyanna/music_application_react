                    "format": "date-time"
                  },
                  "Composer": {
                    "maxLength": 100,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Album": {
                    "maxLength": 100,
                    "minLength": 0,
                    "type": "string"
                  },
